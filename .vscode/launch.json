  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "check status debug",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/node_modules/ts-node/dist/bin.js",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "args": ["${workspaceFolder}/examples/check-status/src/index.ts"],
      "env": {
        "DEBUG": "daf:*"
      }
    },

    {
      "type": "node",
      "request": "launch",
      "name": "test w3c transform",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/node_modules/jest/bin/jest.js",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "args": ["--config=jest.json", "credentialStatus"],
      "env": {
        "DEBUG": "daf:*"
      }
    }
  ]
}
